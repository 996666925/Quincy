rescript = 1
rule cp
  command = cmd.exe /C copy /Y $i $out >NUL
rule touch
 command = cmd.exe /C type nul >>$out & copy $out+,, >NUL
o Demo.cmi : cp ..\bs\QcLib\Demo.cmi
o Demo.cmj : cp ..\bs\QcLib\Demo.cmj
o Demo.cmt : cp ..\bs\QcLib\Demo.cmt
o Demo.res : cp ..\..\QcLib\Demo.res
build install.stamp : touch Demo.cmi Demo.cmj 
